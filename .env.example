# .env.example - Copy to .env and fill in your secrets
# HuggingFace token for model download (required for LLaMA2)
HF_TOKEN=hf_your_access_token_here

# Runpod API key (for client-side requests to the deployed endpoint)
RUNPOD_API_KEY=your_runpod_api_key_here

# Optional: Your deployed endpoint ID (fill in after deployment)
ENDPOINT_ID=your_endpoint_id_here
